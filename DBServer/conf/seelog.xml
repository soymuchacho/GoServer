<seelog minlevel="debug">
	<outputs formatid="main">
		<console/>
		<buffered size="10000" flushperiod="1000">
			<rollingfile type="size" filename="/var/log/DBServer.log" maxsize="100000000" maxrolls="200"/>
		</buffered>
	</outputs>
	<formats>
		<format id="main" format="[%Date(2006-01-02 15:04:05.999 PM MST)] [%Level] [%File.%Line] %Msg%n"/>
	</formats>
</seelog>
